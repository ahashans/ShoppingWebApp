@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm"}))
    {
    @Html.AntiForgeryToken()
        
        <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out"></i>Log off</a></li>    
    }
}
else
{
    
        <li><a href="@Url.Action("Register", "Account")" id = "registerLink"><i class="fa fa-user-plus"></i>Register</a> </li>
    <li><a href="@Url.Action("Login", "Account")" id = "loginLink"><i class="fa fa-sign-in"></i>Login</a></li>
}
